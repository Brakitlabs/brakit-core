{
  "name": "brakit",
  "version": "0.5.11",
  "description": "AI-powered visual code editing tool with real-time web interaction",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "brakit": "./bin/brakit.js"
  },
  "scripts": {
    "dev": "npm run build && npm run start",
    "build": "npm run build:cli && npm run build:backend && npm run build:overlay",
    "build:cli": "tsc -p cli/tsconfig.json",
    "build:backend": "tsc -p backend/tsconfig.json",
    "build:overlay": "cd overlay && npm run build",
    "start": "node dist/cli/index.js",
    "test": "jest",
    "lint": "eslint cli/**/*.ts backend/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "ai",
    "code-editing",
    "cli",
    "development-tools",
    "visual-editing",
    "typescript"
  ],
  "author": "Brakit.ai",
  "license": "MIT",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist/",
    "bin/",
    "backend/dist/",
    "overlay/dist/"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "detect-port": "^1.5.1",
    "diff": "^5.2.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "fs-extra": "^11.3.2",
    "glob": "^11.0.3",
    "http-proxy": "^1.18.1",
    "launch-editor": "^2.9.1",
    "prettier": "^3.6.2",
    "undici": "^6.22.0",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "optionalDependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "inquirer": "^12.9.6",
    "jscodeshift": "^17.3.0",
    "python-shell": "^5.0.0",
    "recast": "^0.23.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/diff": "^5.2.0",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/jscodeshift": "^17.3.0",
    "@types/node": "^24.5.2",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "jest": "^30.1.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
